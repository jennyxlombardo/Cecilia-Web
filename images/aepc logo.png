<!-- verification-code-modal -->
<div id="verification-code-modal" class="auth-modal">
    <div class="sm-modal align-center animate-top">
        <div class="modal-header">
            <img src="./aepc logo_files/modal-header.jpg" alt="modal-header">
            <span class="close-modal" onclick="hide(verificationModal)">×</span>
        </div>
        <div class="modal-body">

            <h4>Verify your account</h4>
            <p class="top-20 text-left" id="toEmail" data-text="We sent an email to [EMAIL] with the verification code. Please enter the code below. ">We sent an email to [EMAIL] with the verification code. Please enter the code below.</p>

            <form name="verification-code-form" onsubmit="submitVerification(event)">
  
                <input id="verification-email" type="hidden" name="email">
  
                <div class="auth-input-group">
                    <label for="code" class="form-label upper">Code </label>
                    <input class="input-box-wrapper" type="number" name="code" id="code" onkeypress="if(this.value.length===6)return false;" onkeyup="validateVerificationCodeTimer(event)">
                    <p class="error-msg">Please enter a valid code. </p>
                    <img class="warning" src="./aepc logo_files/warning.svg">
                </div>
                
                <input type="submit" class="button green input-box-wrapper submit-btn" value="Verify ">
                <p class="top-20 hide" id="regResend">I didn't receive a code! <a class="link green" onclick="resendCode(&#39;reg&#39;)">Re-send again?</a></p>

            </form>
  
            <p class="top-60"><a class="link green" id="back-sign-up">← Back</a></p>
        
        </div>
    </div>
</div>


<!--forgot-password-modal-->
<div id="forgot-modal" class="auth-modal">
    <div class="sm-modal align-center animate-top">
        <div class="modal-header">
            <img src="./aepc logo_files/modal-header.jpg" alt="modal-header">
            <span class="close-modal" onclick="hide(forgotPassModal)">×</span>
        </div>
        <div class="modal-body">

            <h4>Forgotten your password?</h4>
            <p class="top-20 text-left">Please enter your email address to reset your password. </p>

            <form name="forgot-password-form" class="top-35" onsubmit="submitRequestCode(event)">
  
                <div class="auth-input-group">
                    <label for="forgotEmail" class="form-label upper">email</label>
                    <input class="input-box-wrapper" id="forgotEmail" type="email" name="email" onkeyup="validateEmailTimer(event)" onblur="validateEmail(event)">
                    <p class="error-msg">Please enter a valid email format. </p>
                    <img class="warning" src="./aepc logo_files/warning.svg">
                </div>
  
                <input type="submit" class="button green input-box-wrapper submit-btn" value="Request Code ">

            </form>
  
            <div class="top-60"><a class="link green" id="back-login">← Back </a></div>

        </div>
    </div>
</div>


<!-- password-reset-modal -->
