<script>
    function acceptCookieAgreement(){
        fetch('/api/cookie/agree/',{ method: 'POST'})
            .then(response => response.json())
            .then((json)=>{
                if(json.status){
                    document.querySelector('.policies').style.display = 'none';
                }
            })
    }

    function acceptUpdatedAgreements(){
        fetch('/api/agreements/agree/',{ method: 'POST'})
            .then(response => response.json())
            .then((json)=>{
                if(json.status){
                    document.querySelector('.policies').style.display = 'none';
                }
            })
    }
</script>

