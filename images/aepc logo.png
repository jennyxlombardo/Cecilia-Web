<div id="login-modal" class="auth-modal">
    <div class="sm-modal align-center animate-top">
        <div class="modal-header">
            <img src="./aepc logo_files/modal-header.jpg" alt="modal-header">
            <span class="close-modal" onclick="hide(loginModal);changeInputFieldBack()">×</span>
        </div>
        <div class="modal-body">
            <h4>Login</h4>

            <form id="login-form" name="login-form" onsubmit="submitLogin(event)">

                <div class="auth-input-group">
                    <label for="loginEmail" class="form-label upper">email</label>
                    <input class="input-box-wrapper" type="email" name="email" id="loginEmail" onkeyup="validateEmailTimer(event)" value="" onblur="validateEmail(event)">
                    <p class="error-msg">Please enter a valid email format. </p>
                    <img class="warning" src="./aepc logo_files/warning.svg">
                </div>

                <div class="auth-input-group">
                    <label for="loginPass" class="form-label upper">Password</label>
                    <input class="input-box-wrapper" type="password" name="password" id="loginPass" onkeyup="validatePasswordTimer(event)">
                    <p class="error-msg"></p>
                    <img class="eye" src="./aepc logo_files/eye-hide.svg" onclick="showPassword(event)">
                </div>

                <input id="login-btn" type="submit" class="button green input-box-wrapper submit-btn" value="Login">

                <div class="row space-between">
                    <div class="auth-input-group  mobile-left-align">
                        <input type="checkbox" id="rememberMe">
                        <label for="rememberMe">Remember me</label>
                    </div>
                    <div class="auth-input-group  mobile-left-align">
                        <a class="link green bold" id="forgot">Forgot Password? </a>
                    </div>
                </div>

            </form>

            <p class="top-60">Not a member? <a class="link green" id="sign-up">Sign up now! </a></p>
        </div>
